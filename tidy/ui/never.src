fn test(argc: i32) -> i32 {
    return (if true {
        return argc;
    } else {
        argc
    });
}

fn fib(n: isize) -> isize {
    if n <= 1 {
        return n;
    } else {
        fib(n - 1) + fib(n - 2)
    }
}

#[start]
fn main(argc: isize, _: isize) -> isize {
    (fib(argc) == 1 && fib(argc + 1) == 1 && argc == 1).isize - 1
}
